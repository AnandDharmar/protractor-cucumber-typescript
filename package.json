{
  "name": "Cucumber-typescript",
  "version": "1.0.0",
  "license": "MIT",
  "description": "To demostrate protractor cucumber tests for angular/non angular apps with allure report framework",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf allure-report allure-results reports screenshots/actual screenshots/diff",
    "clean-build": "npm run clean && npm run build",
    "init-contributors": "all-contributors init",
    "add-contributors": "all-contributors add",
    "generate-contributors": "all-contributors generate",
    "test": "tsc ./config/config.ts && protractor ./config/config.js --disableChecks",
    "webdriver-update": "webdriver-manager update",
    "allure": "node_modules/.bin/allure generate ./allure-results -c && allure open"
  },
  "author": "Anand Dharmar",
  "devDependencies": {
    "@types/chai-arrays": "^2.0.0",
    "allure-cucumberjs": "2.0.0-beta.7",
    "@types/chai-string": "^1.4.2",
    "allure-commandline": "2.13.0",
    "@types/cucumber": "^4.0.4",
    "@types/node": "^14.14.22",
    "@types/prettier": "^2.1.6",
    "@types/pretty-quick": "^2.0.0",
    "@types/selenium-webdriver": "^4.0.11",
    "all-contributors-cli": "^6.19.0",
    "chai": "^4.2.0",
    "chai-arrays": "2.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-files": "1.4.0",
    "chai-smoothie": "0.3.2",
    "chai-string": "1.5.0",
    "cucumber": "^5.0.3",
    "cucumber-html-reporter": "^4.0.3",
    "mkdirp": "^1.0.4",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0",
    "protractor": "^7.0.0",
    "protractor-axe-html-report-plugin": "1.1.1",
    "protractor-cucumber-framework": "^6.2.1",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "protractor-screenshot-extension": "1.0.7",
    "protractor-multiple-cucumber-html-reporter-plugin": "1.8.1"
  },
  "repository": "https://github.com/AnandDharmar/cucumber-typescript.git"
}
